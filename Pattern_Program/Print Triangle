class Pattern5{
	public static void main(String args[]){
		
	for(int i=0,star=0;i<=5;i++,star+=2){
		for(int space=5;space>i;space--){
			System.out.print(" ");
		}
		for(int j=0;j<=star;j++){
			System.out.print("*");
		}
		System.out.println();
	}
		
	}
}
