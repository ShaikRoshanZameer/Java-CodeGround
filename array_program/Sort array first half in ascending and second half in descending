import java.util.*;
import java.util.Arrays;
class halfAscDes{
	public static void main(String args[]){
		Integer[] arr = {5,4,6,2,1,3,8,9,7};
		
		int n = arr.length;
		
		Arrays.sort(arr,0,n/2);
		Arrays.sort(arr,n/2,n,Collections.reverseOrder());
		System.out.println(Arrays.toString(arr));
		
	}
}
