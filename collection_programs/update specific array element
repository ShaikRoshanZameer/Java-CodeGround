import java.util.*;
class arrayList2{
public static void main(String[] args){
	List<String> ls = new ArrayList<String>();
	ls.add("Red");
	ls.add("yellow");
	ls.add("blue");
	ls.add("purple");
	ls.add("green");
	ls.add("white");
	System.out.println(ls);
	//update
	ls.set(2,"pink");
	System.out.println(ls);
	
}
}
