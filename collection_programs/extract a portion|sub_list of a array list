import java.util.*;
class arrayList6{
public static void main(String[] args){
	List<String> ls = new ArrayList<String>();
	ls.add("red");
	ls.add("yellow");
	ls.add("blue");
	ls.add("purple");
	ls.add("green");
	ls.add("white");
	System.out.println(ls);
	List<String> sub_list = ls.subList(0,3);
	System.out.println("List of first three element: "+sub_list);
	}
}
